{
    "manifest_version": 2,
    "name" : "Programming News",
    "description": "Collects programming news headlines, links, and discussions from various aggregators",
    "version": "1.0",
    "background" : {
      "page": "background.html"
    },
    "browser_action": {
      "default_icon" : "icon.png",
      "default_popup" : "popup.html"
    },
    "permissions": [ 
      "storage",
      "http://news.ycombinator.com/*",
      "http://www.reddit.com/r/programming"
    ]
}